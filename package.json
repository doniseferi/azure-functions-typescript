{
  "name": "azure-functions-typescript",
  "version": "1.0.0",
  "description": "Microsoft serverless Azure Function written in TypeScript.",
  "repository": "https://github.com/janaagaard75/azure-functions-typescript.git",
  "author": "Jan Aagaard <jan@aagaard.net>",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=10.14.1 <11",
    "yarn": ">=1"
  },
  "scripts": {
    "build": "tsc && copyfiles --up 1 \"src/**/*.json\" dist",
    "start": "yarn run build && (cd dist; func host start)",
    "test": "jest",
    "test-save-results": "cross-env JEST_JUNIT_OUTPUT=test-results/test-results.xml jest --ci --runInBand --reporters=default --reporters=jest-junit"
  },
  "devDependencies": {
    "@types/jest": "24.0.13",
    "@types/node": "10.14.*",
    "azure-functions-core-tools": "2.7.1373",
    "copyfiles": "2.1.0",
    "cross-env": "5.2.0",
    "jest": "24.8.0",
    "jest-junit": "6.4.0",
    "ts-jest": "24.0.2",
    "typescript": "3.5.1"
  }
}
